<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" prefHeight="400" prefWidth="600" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="example.controllers.RegistrationController">
    <padding>
        <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
    </padding>
    <Text id="title-text" text=" Welcome!" textAlignment="CENTER" wrappingWidth="267.98633575439453" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="0" />

    <Label text="Username:" GridPane.columnIndex="0" GridPane.rowIndex="1" />

    <TextField fx:id="usernameField" GridPane.columnIndex="1" GridPane.rowIndex="1" />

    <Label text="Password:" GridPane.columnIndex="0" GridPane.rowIndex="2" />

    <PasswordField fx:id="passwordField" GridPane.columnIndex="1" GridPane.rowIndex="2" />

    <Label text="Full Name:" GridPane.columnIndex="0" GridPane.rowIndex="3" />

    <TextField fx:id="fullNameField" GridPane.columnIndex="1" GridPane.rowIndex="3" />

    <Label text="Address:" GridPane.columnIndex="0" GridPane.rowIndex="4" />

    <TextField fx:id="addressField" GridPane.columnIndex="1" GridPane.rowIndex="4" />

    <Label text="E-mail:" GridPane.columnIndex="0" GridPane.rowIndex="5" />

    <TextField fx:id="emailField" GridPane.columnIndex="1" GridPane.rowIndex="5" />

    <Label text="Role:" GridPane.columnIndex="0" GridPane.rowIndex="6" />

    <ChoiceBox fx:id="role" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="6">
    </ChoiceBox>

    <Text fx:id="registrationMessage" GridPane.columnIndex="1" GridPane.rowIndex="7" />
    <HBox alignment="bottom_right" spacing="10" GridPane.columnIndex="1" GridPane.rowIndex="8">
        <Button onAction="#handleRegisterAction" prefHeight="34.0" prefWidth="78.0" text="Register" />
    </HBox>
   <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="9">
     <buttons>
         <Button mnemonicParsing="false" onAction="#onToLogin" prefHeight="37.0" prefWidth="74.0" text="Login" GridPane.columnIndex="1" GridPane.rowIndex="4" />
     </buttons>
   </ButtonBar>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
